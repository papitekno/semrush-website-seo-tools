{
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
  },
  "manifest_version": 2,
  "icons":{
    "128":"128x128.png"
  },
  "name": "Semrush Website SEO Tools",
  "description":"Instantly shows website rankings using extension icons",
  "permissions": [
    "activeTab",
    "webNavigation",
    "https://seoquake.publicapi.semrush.com/*"
  ],
  "version": "1.1.1",
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["content.js"],
      "css":["style.css"]
    }
  ]
}
